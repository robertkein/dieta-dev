/***********Admin tabs*************/
.tabs-wrapper {
  margin: 1px 0 20px;
}
div.tabs { 
	overflow:hidden;
}
	.tabs ul.primary li { margin-bottom:-1px;}
	.tabs ul.primary li a { 
		background:#fff;
		border:1px solid #dcdbcc;
		border-radius:5px 5px 0 0;
		text-shadow:0 1px 0 #FFF;
	}
