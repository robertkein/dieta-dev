/* Drupal 7 Framework
------------------------------------------------*/
body#body {
  background:url(../images/bg.gif) 0 0 repeat fixed;
  color:#575757;
  font-family:Arial, Helvetica, sans-serif;
  font-size:13px;
  line-height:19px;
  margin:0;
  min-width:980px;
  padding:0;
}

p {margin-bottom:15px;}

a { color:#111111; outline:none;}
a:hover {
  color: #b7b498;
  text-decoration: none;
}

#page {}

/*** Header ***/
#header {
	position: relative;
}
.header-wrapper {width:950px; margin:0 auto; position:relative;}
.section-1 {height:139px; background:url(../images/bg-top.png) 0 0 repeat-x; margin-bottom:15px;}
.section-1 .col1 {overflow:hidden; padding-top:8px;}
.section-1 .col2 {float:right; width:336px;}

.section-2 {max-width:1026px; margin:0 auto; min-width:950px;}
.menu-left {float:left; width:300px; margin-right:44px;}

#header #logo {
  float:left;
  margin:0 0px 0 0;
}
#header #site-name-slogan {
  float: left;
}
#header #site-name {
  font-family: Helvetica, Helvetica Neue, Arial, sans-serif;
  font-size:30px;
  font-weight:bold;
  line-height:normal;
  margin:0;
}
#header #site-name a { text-decoration:none}


/*****************Main menu*****************/
.sf-menu { 
	background:none;
	float:right;
	margin:0px;
	position:relative;
	width:auto;
	padding:31px 0 0 0;
}
.sf-menu#superfish-1 {float:left;}
	.block .sf-menu > li {
		background:none !important;
		border:0;
		overflow:inherit;
		float:none;
		font-family:'Trebuchet MS', Arial, Helvetica, sans-serif;
		font-size:16px;
		text-transform:uppercase;
		line-height:19px;
		margin-bottom:7px;
		overflow:visible;
		text-align:center;
	}
	.sf-menu#superfish-2 li {text-align:right;}
		.sf-menu > li > a {
			border:0;
			display:inline-block;
			color:#9b9a91;
			padding:0px 29px 0px 0;
			text-decoration:none;
		}
		.sf-menu > li > a em {
			display:inline-block;
			position:absolute;
			top:0;
			right:0;
			width:20px;
			height:22px;
			background:url(../images/arrows.png) -4px -27px no-repeat;
			}
		.sf-menu#superfish-1 {padding-top:29px;}	
		.sf-menu#superfish-1 > li > a {
			padding:0 0 0 27px;
			}
		.sf-menu#superfish-1 > li {text-align:left;}	
		.sf-menu#superfish-1 > li > a em {
			left:0;
			right:auto;
			top:0;
			background-position:-33px -2px;
			}	
		.sf-menu > li > a:hover, .sf-menu > li > a.active, #superfish-1 > li > a.sfHover{
			color:#dfba48;
		}
		.sf-menu > li > a:hover em, .sf-menu > li > a.active em {background-position:-33px -28px;}
		.sf-menu#superfish-1 > li > a:hover em, .sf-menu#superfish-1 > li > a.active em {background-position:-4px -2px;}
		.sf-menu#superfish-1 > li > a.sfHover em {background-position:-4px -52px;}
	.sf-menu ul {
		background:#f4cd5c;
		border:0px solid #1b1b1b;
		display:none;
		padding:22px 23px;
		top:23px !important;
		border-radius:13px;
		width:120px !important;
		position:absolute !important;
	}
	.sf-navbar > li > ul { margin-top:3px;}
	
		.sf-menu ul li {
			text-align:center;
			background:url(../images/submenu-bg.gif) 0 bottom repeat-x;
			border-bottom:0;
			height:auto;
			padding:0;
			font-size:16px;
			line-height:20px;
			padding-bottom:6px;
			margin-bottom:4px;
			text-transform:uppercase;
		}
		.sf-menu ul li.last {margin-bottom:0; padding-bottom:0; background:none;}
		.sf-menu ul li a {
			border:0;
			padding:0;
			color:#fff;
			font-weight:normal;
			text-decoration:none;
			text-align:left;
		}
		.sf-menu ul li a:hover,.sf-menu ul li a.active {color:#373737;}
	.sf-menu ul li ul{
		left:160px !important;
		padding:10px 0 !important;
		top:0 !important;
	}
	.sf-menu ul li ul li {
		background:none;
		margin-bottom:1px;
	}
